{
  "name": "langserve-backend",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "examples/langserve-backend/src",
  "targets": {
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "command": "uv run serve",
        "cwd": "examples/langserve-backend"
      }
    },
    "dev": {
      "executor": "nx:run-commands",
      "options": {
        "command": "uv run uvicorn svelte_langserve.main:create_app --factory --reload --port 8000",
        "cwd": "examples/langserve-backend"
      }
    },
    "start": {
      "executor": "nx:run-commands",
      "options": {
        "command": "uv run main.py",
        "cwd": "examples/langserve-backend"
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "uv run pytest",
        "cwd": "examples/langserve-backend"
      }
    },
    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "command": "uv run ruff check .",
        "cwd": "examples/langserve-backend"
      }
    },
    "format": {
      "executor": "nx:run-commands",
      "options": {
        "command": "uv run ruff format .",
        "cwd": "examples/langserve-backend"
      }
    },
    "typecheck": {
      "executor": "nx:run-commands",
      "options": {
        "command": "uv run mypy src/",
        "cwd": "examples/langserve-backend"
      }
    }
  },
  "tags": [
    "type:app",
    "scope:backend"
  ]
}
