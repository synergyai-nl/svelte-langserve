{
  "name": "langserve-backend",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "examples/langserve-backend/src",
  "targets": {
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "command": "uv run serve",
        "cwd": "examples/langserve-backend"
      }
    },
    "dev": {
      "executor": "nx:run-commands",
      "options": {
        "command": "uv run uvicorn src.claude_dashboard_backend.main:create_app --factory --reload --port 8000",
        "cwd": "examples/langserve-backend"
      }
    },
    "start": {
      "executor": "nx:run-commands",
      "options": {
        "command": "uv run main.py",
        "cwd": "examples/langserve-backend"
      }
    }
  },
  "tags": ["type:app", "scope:backend"]
}