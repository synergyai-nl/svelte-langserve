{
	"name": "svelte-langserve",
	"$schema": "../../node_modules/nx/schemas/project-schema.json",
	"projectType": "library",
	"sourceRoot": "packages/svelte-langserve/src",
	"targets": {
		"build": {
			"executor": "nx:run-commands",
			"options": {
				"command": "svelte-package",
				"cwd": "packages/svelte-langserve"
			},
			"outputs": ["{projectRoot}/dist"]
		},
		"dev": {
			"executor": "nx:run-commands",
			"options": {
				"command": "svelte-package --watch",
				"cwd": "packages/svelte-langserve"
			}
		},
		"check": {
			"executor": "nx:run-commands",
			"options": {
				"command": "svelte-check --tsconfig ./tsconfig.json",
				"cwd": "packages/svelte-langserve"
			}
		},
		"lint": {
			"executor": "nx:run-commands",
			"options": {
				"command": "eslint .",
				"cwd": "packages/svelte-langserve"
			}
		},
		"test": {
			"executor": "nx:run-commands",
			"options": {
				"command": "vitest run",
				"cwd": "packages/svelte-langserve"
			}
		},
		"test:watch": {
			"executor": "nx:run-commands",
			"options": {
				"command": "vitest",
				"cwd": "packages/svelte-langserve"
			}
		},
		"clean": {
			"executor": "nx:run-commands",
			"options": {
				"command": "rm -rf dist",
				"cwd": "packages/svelte-langserve"
			}
		}
	}
}