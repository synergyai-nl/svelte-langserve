{
  "name": "@svelte-langserve/codegen",
  "version": "0.1.0",
  "description": "Type generation tooling for svelte-langserve packages",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "svelte-langserve-codegen": "dist/cli.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "schemas"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@svelte-langserve/types": "workspace:*",
    "json-schema-to-typescript": "^13.1.2",
    "commander": "^14.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "typescript": "^5.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}