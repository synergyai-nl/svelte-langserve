{
  "name": "@svelte-langserve/core",
  "version": "0.1.0",
  "description": "Core integration package for connecting Svelte apps to LangServe backends",
  "type": "module",
  "main": "dist/index.js",
  "svelte": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js",
      "import": "./dist/index.js"
    },
    "./stores": {
      "types": "./dist/stores/index.d.ts",
      "svelte": "./dist/stores/index.js",
      "import": "./dist/stores/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "svelte-package",
    "dev": "svelte-package --watch",
    "clean": "rm -rf dist",
    "typecheck": "svelte-check --tsconfig ./tsconfig.json"
  },
  "peerDependencies": {
    "svelte": "^5.0.0",
    "socket.io-client": "^4.8.0"
  },
  "dependencies": {
    "@svelte-langserve/types": "workspace:*"
  },
  "devDependencies": {
    "@sveltejs/package": "^2.3.0",
    "svelte": "^5.0.0",
    "svelte-check": "^4.0.0",
    "typescript": "^5.0.0",
    "socket.io-client": "^4.8.0"
  },
  "publishConfig": {
    "access": "public"
  }
}