{
  "name": "@svelte-langserve/ui",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "packages/@svelte-langserve/ui/src",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "svelte-package",
        "cwd": "packages/@svelte-langserve/ui"
      }
    },
    "dev": {
      "executor": "nx:run-commands",
      "options": {
        "command": "svelte-package --watch",
        "cwd": "packages/@svelte-langserve/ui"
      }
    },
    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "command": "eslint .",
        "cwd": "packages/@svelte-langserve/ui"
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "vitest run",
        "cwd": "packages/@svelte-langserve/ui"
      }
    },
    "test:watch": {
      "executor": "nx:run-commands",
      "options": {
        "command": "vitest",
        "cwd": "packages/@svelte-langserve/ui"
      }
    },
    "typecheck": {
      "executor": "nx:run-commands",
      "options": {
        "command": "svelte-check --tsconfig ./tsconfig.json",
        "cwd": "packages/@svelte-langserve/ui"
      }
    },
    "clean": {
      "executor": "nx:run-commands",
      "options": {
        "command": "rm -rf dist",
        "cwd": "packages/@svelte-langserve/ui"
      }
    }
  },
  "tags": ["type:lib", "scope:ui"]
}