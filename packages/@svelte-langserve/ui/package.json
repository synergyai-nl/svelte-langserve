{
  "name": "@svelte-langserve/ui",
  "version": "0.1.0",
  "description": "Pre-built UI components for svelte-langserve",
  "type": "module",
  "main": "dist/index.js",
  "svelte": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js",
      "import": "./dist/index.js"
    },
    "./components": {
      "types": "./dist/components/index.d.ts",
      "svelte": "./dist/components/index.js",
      "import": "./dist/components/index.js"
    },
    "./styles": "./dist/styles.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "svelte-package",
    "dev": "svelte-package --watch",
    "clean": "rm -rf dist",
    "typecheck": "svelte-check --tsconfig ./tsconfig.json"
  },
  "peerDependencies": {
    "svelte": "^5.0.0"
  },
  "dependencies": {
    "@svelte-langserve/core": "workspace:*",
    "@svelte-langserve/types": "workspace:*"
  },
  "devDependencies": {
    "@sveltejs/package": "^2.3.0",
    "svelte": "^5.0.0",
    "svelte-check": "^4.0.0",
    "typescript": "^5.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}